Node Presence_Virus {
  logic = Presence_Virus;
}

Node Eating {
  logic = Presence_Virus;
  rate_up  = @logic ? $eating_activation : 0; 
  rate_down  = @logic ? 0 : $eating_inactivation;
}

Node Release_Cytokines {
  logic = Presence_Virus;
  rate_up  = @logic ? $cytokin_activation : 0; 
  rate_down  = @logic ? 0 : $cytokin_inactivation;
}
